public class Palindrome {
	public static void main(String[] args) {
		String s1 = "tlevel";
		String s2 = "";
		for (int i = 0; i < s1.length(); i++) 
		{
			char c = s1.charAt(i);
			s2 = s2 + c;

		}
		if (s1.equals(s2)) {
			System.out.println("Palindrome");
		} else {
			System.out.println("Not A palindrome");
		}
	}
}
